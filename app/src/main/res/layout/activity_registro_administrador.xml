<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/registros"
    tools:context=".RegistroAdministrador">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="410dp"
        android:layout_height="101dp"
        android:background="#702FF321"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/imageAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/registroAdmin"
            android:layout_width="364dp"
            android:layout_height="68dp"
            android:layout_weight="100"
            android:gravity="center"
            android:rotationX="8"
            android:text="Registro Administrador"
            android:textColor="@color/black"
            android:textSize="24sp"
            android:textStyle="bold" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:text="Nombre"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        app:layout_constraintVertical_bias="0.806" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="28dp"
        android:text="Celular"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="40dp"
        android:text="E-mail"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="44dp"
        android:text="Usuario"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="44dp"
        android:text="Contraseña"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="32dp"
        android:text="Identificación"
        android:textColor="@color/white"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="104dp"
        android:text="Edad"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/nameAdmin"
        android:layout_width="205dp"
        android:layout_height="28dp"
        android:layout_marginBottom="28dp"
        android:background="#FFFFFF"
        android:hint=""
        app:layout_constraintBottom_toTopOf="@+id/celAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toEndOf="@+id/textView6" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/celAdmin"
        android:layout_width="206dp"
        android:layout_height="27dp"
        android:layout_marginBottom="32dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:hint=""
        app:layout_constraintBottom_toTopOf="@+id/emailAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toEndOf="@+id/textView7" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/emailAdmin"
        android:layout_width="206dp"
        android:layout_height="27dp"
        android:layout_marginBottom="32dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:hint=""
        app:layout_constraintBottom_toTopOf="@+id/userAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toEndOf="@+id/textView8" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/userAdmin"
        android:layout_width="206dp"
        android:layout_height="27dp"
        android:layout_marginBottom="36dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:hint=""
        app:layout_constraintBottom_toTopOf="@+id/passwordAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toEndOf="@+id/textView9" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/passwordAdmin"
        android:layout_width="206dp"
        android:layout_height="27dp"
        android:layout_marginBottom="28dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:hint=""
        app:layout_constraintBottom_toTopOf="@+id/idAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.404"
        app:layout_constraintStart_toEndOf="@+id/textView10" />


    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/idAdmin"
        android:layout_width="205dp"
        android:layout_height="28dp"
        android:layout_marginBottom="24dp"
        android:background="#FFFFFF"
        android:hint=""
        app:layout_constraintBottom_toTopOf="@+id/ageAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.307"
        app:layout_constraintStart_toEndOf="@+id/textView11" />

    <com.google.android.material.textfield.TextInputEditText
        android:id="@+id/ageAdmin"
        android:layout_width="206dp"
        android:layout_height="27dp"
        android:layout_marginBottom="32dp"
        android:background="#FFFFFF"
        android:gravity="center"
        android:hint=""
        app:layout_constraintBottom_toTopOf="@+id/buttonRegisterAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.573"
        app:layout_constraintStart_toEndOf="@+id/textView12" />

    <Button
        android:id="@+id/buttonRegisterAdmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:text="Registrar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cancelRegisterAdmin"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/cancelRegisterAdmin"
        android:layout_width="116dp"
        android:layout_height="51dp"
        android:layout_marginBottom="24dp"
        android:text="Cancelar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.82"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageAdmin"
        android:layout_width="112dp"
        android:layout_height="115dp"
        android:layout_marginStart="148dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toTopOf="@+id/nameAdmin"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/buttonImage"
        android:layout_width="104dp"
        android:layout_height="36dp"
        android:layout_marginBottom="64dp"
        android:text="Imagen"
        app:layout_constraintBottom_toTopOf="@+id/nameAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.529"
        app:layout_constraintStart_toEndOf="@+id/imageAdmin" />
</androidx.constraintlayout.widget.ConstraintLayout>